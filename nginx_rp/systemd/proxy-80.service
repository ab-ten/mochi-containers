[Unit]
Description=nginx_rp HTTP Proxy via systemd-socket-proxyd
Documentation=man:systemd-socket-proxyd(1)
Requires=@@ROOT_UNIT_PREFIX@@proxy-80.socket
After=@@ROOT_UNIT_PREFIX@@proxy-80.socket

[Service]
#Type=notify
ExecStart=/usr/lib/systemd/systemd-socket-proxyd 127.0.0.1:8080
PrivateTmp=yes
